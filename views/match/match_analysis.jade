extend match

block match_content
  .row
    .col-md-12
      +data_table({id:"analysis", heading: "Analysis"})
        each player, i in match.players
          +data_table_header(i, match)
            tr
              th: abbr(title=tooltips.hero_id) Hero
              th Analysis
              //-
                each val, key in player.analysis
                  th=key
          tr(class = player.isRadiant ? "radiant" : "dire")
            +hero_td(player)
            //-
              each val, key in player.analysis
                td
                  =val && val.condition ? val.value : "n"
            //-
            td
              each val, key in player.analysis
                if val
                  span.btn(class=!val.condition ? "btn-"+val.category : "")
                    span: abbr(title=util.format.apply(null, [val.template].concat(val.value))+"<br>"+val.advice)
                      span.fa(style="margin:3px;", class=val.icon)