extend player

block playercontent
  .row
    .col-md-6
      h3 Multi Kills 
        small (#{player.aggData.multi_kills.n} matches)
      table.table.table-striped
        thead
          tr
            th Type
            th Count
        tbody
          each val, key in constants.multi_kills
            tr
              td #{val} (#{key})
              td=player.aggData.multi_kills.counts[key] || 0
  
          