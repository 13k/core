extend layout

block content
  //pre=JSON.stringify(matches)
  .row
    .col-md-12
      table.table
        th ID
        th League
        th Radiant
        th Dire
        each match in matches
          tr
            td: a(href="/matches/"+match.match_id)=match.match_id
            td=match.leagueid
            td=match.radiant_name
            td=match.dire_name
