{
  "name": "yasp",
  "description": "Open source Dota 2 replay parsing and statistics",
  "version": "5.0.0",
  "main": "index.js",
  "scripts":
  {
    "start": "node deploy",
    "dev": "nf -j Procfile.dev start",
    "dev2": "pm2-dev deploy.json",
    "test": "test/run.sh",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "update": "npm-check-updates -u",
    "webpack": "webpack -p",
    "watch": "webpack --watch",
    "mocha": "mocha",
    "maven": "mvn -q -f java_parser/pom.xml clean install -U",
    "build": "npm install && npm run maven && npm run webpack",
    "startup": "sudo pm2 startup ubuntu -u yasp",
    "create": "bash sql/create.sh",
    "psql": "psql -d postgresql://yasp:yasp@localhost/yasp",
    "deploy": "kubectl get rc -o name --selector='tier' | cut -d '/' -f2 | xargs kubectl rolling-update --image=yasp/yasp:latest",
    "deploy-core": "git pull origin master && npm run build && node deploy core",
    "deploy-parser": "git pull origin master && npm run maven && node deploy parser"
  },
  "repository":
  {
    "type": "git",
    "url": "http://github.com/yasp-dota/yasp"
  },
  "dependencies":
  {
    "JSONStream": "^1.0.7",
    "async": "1.5.0",
    "big-number": "0.3.1",
    "body-parser": "1.14.1",
    "bootstrap": "^3.3.6",
    "bootswatch": "^3.3.6",
    "bull": "yasp-dota/bull",
    "cal-heatmap": "^3.5.4",
    "compression": "^1.6.0",
    "connect-redis": "^3.0.1",
    "cookie-session": "^2.0.0-alpha",
    "d3": "^3.5.9",
    "datatables": "^1.10.9",
    "datatables-bootstrap3-plugin": "^0.4.0",
    "dota2": "~1.0.1",
    "dota2-minimap-hero-sprites": "~1.0.1",
    "dotenv": "^1.2.0",
    "express": "~4.13.3",
    "express-session": "~1.12.1",
    "font-awesome": "^4.4.0",
    "foreman": "~1.4.1",
    "heatmap.js": "~2.0.1",
    "http-proxy": "^1.12.0",
    "jade": "~1.11.0",
    "jquery": "^2.1.4",
    "knex": "^0.9.0",
    "moment": "^2.10.6",
    "ndjson": "^1.4.3",
    "numeral": "^1.5.3",
    "passport": "0.3.2",
    "passport-steam": "0.1.6",
    "paypal-rest-sdk": "~1.6",
    "pg": "^4.4.3",
    "pg-query-stream": "^1.0.0",
    "pm2": "^0.15.10",
    "poet": "~2.0.1",
    "qTip2": "yasp-dota/qtip2",
    "redis": "2.4.2",
    "request": "~2.67.0",
    "request-progress": "^0.3.1",
    "sentiment": "^1.0.2",
    "steam": "1.3.0",
    "winston": "2.1.1",
    "wordcloud": "1.0.1"
  },
  "devDependencies":
  {
    "coveralls": "2.11.4",
    "css-loader": "^0.23.0",
    "database-cleaner": "~0.10.1",
    "file-loader": "^0.8.5",
    "istanbul": "~0.4.1",
    "json-loader": "^0.5.4",
    "mocha": "~2.3.4",
    "nock": "~3.3.2",
    "nodemon": "^1.8.1",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  },
  "engines":
  {
    "node": "5.0.0"
  }
}
