{
  "name": "yasp",
  "description": "Yet Another (Dota) Stats Page",
  "version": "1.5.0",
  "scripts": {
    "start": "npm run retriever",
    "retriever": "nf -j Procfile.retriever start",
    "parser": "npm run build && nf -j Procfile.parser start",
    "dev": "nf -j Procfile.dev start",
    "test": "npm install && npm run build && istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "prod": "npm install && npm run build && redis-cli KEYS 'match:*' | xargs -n 100 redis-cli DEL && sudo nf export -j Procfile.prod -o /etc/init/ && sudo restart foreman",
    "mocha": "mocha",
    "bower": "bower install --allow-root",
    "unparsed": "node runner.js unparsed",
    "constants": "node runner.js constants",
    "fullhistory": "node runner.js fullhistory",
    "updatenames": "node runner.js updatenames",
    "getmissing": "node runner.js getmissing",
    "browserify": "browserify public/js/browser.js | uglifyjs > public/js/bundle.js",
    "watchify": "watchify public/js/browser.js -o public/js/bundle.js --debug --verbose",
    "build": "mvn -q -f parser/pom.xml package",
    "postinstall": "npm run bower && npm run browserify && npm run constants"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/yasp-dota/yasp"
  },
  "dependencies": {
    "JSONStream": "^0.10.0",
    "async": "0.9.0",
    "aws-sdk": "~2.1.17",
    "big-number": "0.3.1",
    "body-parser": "1.12.0",
    "bower": "^1.3.12",
    "browserify": "~9.0.3",
    "c3": "~0.4.10-rc4",
    "cal-heatmap": "~3.5.2",
    "compression": "^1.4.3",
    "connect-redis": "^2.2.0",
    "datatables": "~1.10.5",
    "dota2": "RJacksonm1/node-dota2",
    "dotenv": "~1.0.0",
    "express": "~4.12.2",
    "express-session": "~1.10.3",
    "foreman": "~1.3.0",
    "heatmap.js": "howardc93/heatmap.js",
    "http-auth": "~2.2.5",
    "jade": "~1.9.2",
    "jquery": "~2.1.3",
    "kue": "~0.8.11",
    "moment": "~2.9.0",
    "monk": "1.0.0",
    "multer": "~0.1.8",
    "multiparty": "^4.1.1",
    "numeral": "~1.5.3",
    "parse-redis-url": "0.0.2",
    "passport": "0.2.1",
    "passport-steam": "0.1.4",
    "paypal-rest-sdk": "~1.5",
    "poet": "~2.0.0",
    "qtip2": "~2.2.0",
    "recaptcha": "1.2.0",
    "redis": "0.12.1",
    "request": "~2.53.0",
    "request-progress": "^0.3.1",
    "seaport": "^2.0.9",
    "socket.io": "^1.3.5",
    "steam": "0.6.8",
    "uglify-js": "~2.4.17",
    "winston": "0.9.0"
  },
  "devDependencies": {
    "coveralls": "2.11.2",
    "database-cleaner": "~0.9.1",
    "istanbul": "~0.3.7",
    "mocha": "~2.2.1",
    "mongodb": "~1.4.34",
    "nock": "~1.1.0",
    "nodemon": "^1.3.7",
    "supertest": "^0.15.0",
    "watchify": "~2.4.0",
    "zombie": "~2.5.1"
  }
}
