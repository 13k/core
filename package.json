{
  "name": "yasp",
  "description": "YASP: Another (Dota) Stats Page",
  "version": "3.1.0",
  "main": "index.js",
  "scripts": {
    "start": "nf run node index.js",
    "test": "rm -rf testfiles && git clone https://github.com/yasp-dota/testfiles && npm run build && istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "unparsed": "nf run node runner.js unparsed",
    "updateNames": "nf run node runner.js updateNames",
    "buildSets": "nf run node runner.js buildSets",
    "constants": "nf run node runner.js constants",
    "fullhistory": "nf run node runner.js fullhistory",
    "shorthistory": "nf run node runner.js shorthistory",
    "getmissing": "nf run node runner.js getmissing",
    "webpack": "webpack -p",
    "watch": "webpack --watch",
    "build": "npm install && npm run webpack && npm run maven && npm run constants",
    "maven": "mvn -q -f parser/pom.xml clean install -U",
    "dev": "nf -j Procfile.dev start",
    "yasp-parser": "sudo nf export -j Procfile.prod -o /etc/init/ parser && sudo restart foreman",
    "yasp-core": "sudo nf export -j Procfile.prod -o /etc/init/ web,worker,parser,parseManager,scanner && sudo restart foreman"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/yasp-dota/yasp"
  },
  "dependencies": {
    "JSONStream": "0.10.0",
    "async": "1.2.0",
    "big-number": "0.3.1",
    "body-parser": "1.12.4",
    "bootstrap": "^3.3.4",
    "c3": "^0.4.10",
    "cal-heatmap": "^3.5.2",
    "compression": "^1.4.4",
    "connect-redis": "^2.3.0",
    "d3": "^3.5.5",
    "datatables": "^1.10.7",
    "datatables-bootstrap3-plugin": "^0.4.0",
    "dota2": "yasp-dota/node-dota2",
    "dota2-minimap-hero-sprites": "yasp-dota/dota2-minimap-hero-sprites",
    "dotenv": "~1.1.0",
    "express": "~4.12.4",
    "express-session": "~1.11.2",
    "font-awesome": "^4.3.0",
    "foreman": "~1.4.0",
    "heatmap.js": "pa7/heatmap.js#develop",
    "http-auth": "~2.2.8",
    "http-proxy": "^1.11.1",
    "jade": "~1.10.0",
    "jquery": "^2.1.4",
    "kue": "~0.9.3",
    "moment": "^2.10.3",
    "monk": "1.0.1",
    "numeral": "^1.5.3",
    "parse-redis-url": "0.0.2",
    "passport": "0.2.2",
    "passport-steam": "0.1.5",
    "paypal-rest-sdk": "~1.5",
    "poet": "~2.0.1",
    "qTip2": "yasp-dota/qtip2",
    "redis": "0.12.1",
    "request": "~2.57.0",
    "request-progress": "^0.3.1",
    "select2": "^3.5.2-browserify",
    "sentiment": "^1.0.2",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "steam": "0.6.8",
    "vis": "^4.2.0",
    "winston": "1.0.0",
    "wordcloud2.js": "yasp-dota/wordcloud2.js"
  },
  "devDependencies": {
    "coveralls": "2.11.2",
    "css-loader": "^0.14.4",
    "database-cleaner": "~0.9.1",
    "file-loader": "^0.8.4",
    "istanbul": "~0.3.14",
    "json-loader": "^0.5.2",
    "mocha": "~2.2.5",
    "mongodb": "~2.0.33",
    "nock": "~2.4.0",
    "nodemon": "^1.3.7",
    "style-loader": "^0.12.3",
    "supertest": "^1.0.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.10"
  },
  "engines": {
    "node": ">= 0.12.0"
  }
}
