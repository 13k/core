{
  "name": "yasp",
  "description": "YASP: Another (Dota) Stats Page",
  "version": "3.0.0",
  "scripts": {
    "start": "npm run retriever",
    "retriever": "nf -j Procfile.retriever start",
    "dev": "nf -j Procfile.dev start",
    "test": "npm run maven && istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "unparsed": "node runner.js unparsed",
    "constants": "node runner.js constants",
    "updateNames": "node runner.js updateNames",
    "buildSets": "node runner.js buildSets",
    "fullhistory": "node runner.js fullhistory",
    "shorthistory": "node runner.js shorthistory",
    "getmissing": "node runner.js getmissing",
    "bower": "bower install",
    "grunt": "grunt",
    "build": "npm install && npm run maven",
    "postinstall": "npm run bower && npm run constants && npm run grunt",
    "maven": "mvn -q -f parser/pom.xml clean install -U",
    "yasp-parser": "sudo nf export -j Procfile.parser -o /etc/init/ && sudo restart foreman",
    "yasp-core": "sudo nf export -j Procfile.prod -o /etc/init/ && sudo restart foreman"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/yasp-dota/yasp"
  },
  "dependencies": {
    "JSONStream": "^0.10.0",
    "async": "0.9.0",
    "aws-sdk": "~2.1.21",
    "big-number": "0.3.1",
    "body-parser": "1.12.2",
    "bower": "^1.4.1",
    "compression": "^1.4.3",
    "connect-redis": "^2.2.0",
    "dota2": "yasp-dota/node-dota2",
    "dotenv": "~1.1.0",
    "express": "~4.12.3",
    "express-session": "~1.10.4",
    "foreman": "~1.3.0",
    "http-auth": "~2.2.5",
    "jade": "~1.9.2",
    "kue": "~0.8.12",
    "moment": "~2.9.0",
    "monk": "1.0.1",
    "parse-redis-url": "0.0.2",
    "passport": "0.2.1",
    "passport-steam": "0.1.4",
    "paypal-rest-sdk": "~1.5",
    "poet": "~2.0.0",
    "redis": "0.12.1",
    "request": "~2.55.0",
    "request-progress": "^0.3.1",
    "seaport": "^2.0.9",
    "sentiment": "^1.0.2",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "steam": "0.6.8",
    "winston": "0.9.0"
  },
  "devDependencies": {
    "coveralls": "2.11.2",
    "database-cleaner": "~0.9.1",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-cssmin": "^0.12.2",
    "grunt-contrib-jshint": "^0.11.1",
    "grunt-contrib-uglify": "^0.8.1",
    "istanbul": "~0.3.13",
    "mocha": "~2.2.1",
    "mongodb": "~2.0.25",
    "nock": "~1.4.0",
    "nodemon": "^1.3.7",
    "supertest": "^0.15.0"
  }
}
