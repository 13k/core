{
  "name": "yasp",
  "description": "Open source Dota 2 replay parsing and statistics",
  "version": "5.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node deploy",
    "dev": "nf -j Procfile.dev start",
    "dev2": "pm2-dev deploy.json",
    "test": "rm -rf testfiles && git clone https://github.com/yasp-dota/testfiles && npm run build && istanbul cover _mocha --report lcovonly -- -R spec",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "constants": "node constantsUpdate.js",
    "update": "npm-check-updates -u",
    "webpack": "webpack -p",
    "watch": "webpack --watch",
    "mocha": "mocha",
    "maven": "mvn -q -f java_parser/pom.xml clean install -U",
    "build": "npm install && npm run maven && npm run webpack",
    "startup": "sudo pm2 startup ubuntu -u yasp",
    "create": "bash sql/create.sh",
    "deploy-core": "node deploy core",
    "deploy-parser": "git pull origin master && npm run maven && node deploy parser"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/yasp-dota/yasp"
  },
  "dependencies": {
    "async": "1.4.2",
    "big-number": "0.3.1",
    "body-parser": "1.14.1",
    "bootstrap": "^3.3.5",
    "bootswatch": "^3.3.5",
    "bull": "^0.6.0",
    "cal-heatmap": "^3.5.4",
    "compression": "^1.6.0",
    "connect-redis": "^3.0.0",
    "cookie-session": "^1.2.0",
    "d3": "^3.5.6",
    "datatables": "^1.10.9",
    "datatables-bootstrap3-plugin": "^0.4.0",
    "dota2": "yasp-dota/node-dota2#dev",
    "dota2-minimap-hero-sprites": "~1.0.1",
    "dotenv": "^1.2.0",
    "express": "~4.13.3",
    "express-session": "~1.11.3",
    "font-awesome": "^4.4.0",
    "foreman": "~1.4.1",
    "heatmap.js": "~2.0.1",
    "http-proxy": "^1.11.2",
    "jade": "~1.11.0",
    "jquery": "^2.1.4",
    "knex": "^0.8.6",
    "moment": "^2.10.6",
    "ndjson": "^1.4.2",
    "numeral": "^1.5.3",
    "passport": "0.3.0",
    "passport-steam": "0.1.6",
    "paypal-rest-sdk": "~1.6",
    "pg": "^4.4.2",
    "pm2": "^0.15.7",
    "poet": "~2.0.1",
    "qTip2": "yasp-dota/qtip2",
    "redis": "2.1.0",
    "request": "~2.64.0",
    "request-progress": "^0.3.1",
    "sentiment": "^1.0.2",
    "steam": "1.2.0",
    "winston": "1.0.2",
    "wordcloud": "1.0.1"
  },
  "devDependencies": {
    "coveralls": "2.11.4",
    "css-loader": "^0.19.0",
    "database-cleaner": "~0.10.0",
    "file-loader": "^0.8.4",
    "istanbul": "~0.3.22",
    "json-loader": "^0.5.3",
    "mocha": "~2.3.3",
    "mongodb": "~2.0.45",
    "nock": "~2.13.0",
    "nodemon": "^1.7.1",
    "style-loader": "^0.12.4",
    "supertest": "^1.1.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2"
  },
  "engines": {
    "node": "4.1.1"
  }
}
